<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.CheckBox?>
<GridPane
    xmlns="http://javafx.com/javafx"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="nathan.apes.tasksynth.ui.rundialog.RunDialogGUI"
    prefHeight="300" prefWidth="300"
    gridLinesVisible="true">

    <columnConstraints>
        <ColumnConstraints minWidth="300"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="20"/>
    </rowConstraints>

    <background>
        <Background>
            <fills><BackgroundFill fill="white"/></fills>
        </Background>
    </background>

    <CheckBox id="0" GridPane.rowIndex="0"/>
    <CheckBox id="1" GridPane.rowIndex="1"/>
    <CheckBox id="2" GridPane.rowIndex="2"/>
    <CheckBox id="3" GridPane.rowIndex="3"/>
    <CheckBox id="4" GridPane.rowIndex="4"/>
    <CheckBox id="5" GridPane.rowIndex="5"/>
    <CheckBox id="6" GridPane.rowIndex="6"/>
    <CheckBox id="7" GridPane.rowIndex="7"/>
    <CheckBox id="8" GridPane.rowIndex="8"/>
    <CheckBox id="9" GridPane.rowIndex="9"/>
    <CheckBox id="10" GridPane.rowIndex="10"/>
    <CheckBox id="11" GridPane.rowIndex="11"/>
    <CheckBox id="12" GridPane.rowIndex="12"/>
    <CheckBox id="13" GridPane.rowIndex="13"/>
    <CheckBox id="14" GridPane.rowIndex="14"/>
    <CheckBox id="15" GridPane.rowIndex="15"/>
    <Group GridPane.rowIndex="16" translateX="70">
        <Button text="Run" onAction="#onRunSubmit"/>
        <Button text="Cancel" translateX="100" onAction="#onCancel"/>
    </Group>
</GridPane>
